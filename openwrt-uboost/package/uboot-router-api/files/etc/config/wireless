config wifi-device 'radio0'
    option type 'mac80211'
    option path 'platform/10300000.wmac'
    option channel 'auto'
    option band '2g'
    option htmode 'HT20'
    option disabled '0'
    option country 'RU'

config wifi-iface 'default_radio0'
    option device 'radio0'
    option network 'lan'
    option mode 'ap'
    option ssid 'UBoost-WiFi'
    option encryption 'psk2'
    option key 'uboot12345678'
    option isolate '0'

config wifi-device 'radio1'
    option type 'mac80211'
    option path 'pci0000:00/0000:00:01.0/0000:01:00.0'
    option channel 'auto'
    option band '5g'
    option htmode 'VHT80'
    option disabled '0'
    option country 'RU'

config wifi-iface 'default_radio1'
    option device 'radio1'
    option network 'lan'
    option mode 'ap'
    option ssid 'UBoost-WiFi-5G'
    option encryption 'psk2'
    option key 'uboot12345678'
    option isolate '0'
