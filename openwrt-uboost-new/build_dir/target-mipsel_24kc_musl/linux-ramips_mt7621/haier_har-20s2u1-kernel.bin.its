/dts-v1/;

/ {
	description = "MIPS OpenWrt FIT (Flattened Image Tree)";
	#address-cells = <1>;

	images {
		kernel-1 {
			description = "MIPS OpenWrt Linux-5.15.150";
			data = /incbin/("/home/notlean/openwrt-uboost/build_dir/target-mipsel_24kc_musl/linux-ramips_mt7621/haier_har-20s2u1-kernel.bin");
			type = "kernel";
			arch = "mips";
			os = "linux";
			compression = "lzma";
			load = <0x82000000>;
			entry = <0x82000000>;
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};


		fdt-1 {
			description = "MIPS OpenWrt haier_har-20s2u1 device tree blob";
			
			data = /incbin/("/home/notlean/openwrt-uboost/build_dir/target-mipsel_24kc_musl/linux-ramips_mt7621/image-mt7621_haier_har-20s2u1.dtb");
			type = "flat_dt";
			
			arch = "mips";
			compression = "none";
			hash-1 {
				algo = "crc32";
			};
			hash-2 {
				algo = "sha1";
			};
		};



	};

	configurations {
		default = "config-1";
		config-1 {
			description = "OpenWrt haier_har-20s2u1";
			kernel = "kernel-1";
			fdt = "fdt-1";
			
			
			
		};
		
	};
};
